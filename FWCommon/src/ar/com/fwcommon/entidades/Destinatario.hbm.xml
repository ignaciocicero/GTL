<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
   <class name="ar.com.fwcommon.entidades.Destinatario" table="T_Destinatario">
      <id name="idDestinatario" column="P_IdDestinatario" type="int" unsaved-value="0">
         <generator class="identity"/>
      </id>
      <property name="tipoDestinatario" column="A_TipoDestinatario" type="int" />
      <property name="direccionMail" column="A_DireccionMail" type="string" />
   </class>
</hibernate-mapping>
