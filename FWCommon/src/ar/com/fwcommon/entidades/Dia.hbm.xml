<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">
	<class name="ar.com.fwcommon.entidades.Dia" table="T_Dia">
		<id name="idDia" column="P_IdDia" type="int" unsaved-value="0">
			<generator class="identity"/>
		</id>
		<property name="nombre" column="A_Nombre" type="string"/>
		<property name="habil" column="A_DiaHabil" type="boolean"/>
		<property name="nroDia" column="A_NumeroDia" type="int"/>
	</class>
</hibernate-mapping>